{
  "expo": {
    "name": "Diversity",
    "slug": "Diversity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/diversity_logo_black.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#343DD0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diversity.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/diversity_logo_black.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/diversity_logo_black.png"
    }
  }
}
